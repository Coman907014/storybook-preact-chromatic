{
  "name": "storybook-preact-chromatic",
  "version": "1.0.9",
  "license": "MIT",
  "main": "public/index.js",
  "module": "public/index.esm.js",
  "files": [
    "public"
  ],
  "scripts": {
    "build": "preact build --no-prerender",
    "build-for-publish": "set NODE_ENV=production && rollup -c rollup.config.ts",
    "serve": "sirv build --port 8080 --cors --single",
    "eject": "preact eject",
    "release": "auto shipit --base-branch=main",
    "compile-less": "concurrently --kill-others \"less-watch-compiler --config less-watcher.config.json\"",
    "dev": "preact watch",
    "lint": "eslint src",
    "lint-fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "chromatic": "npx chromatic --project-token=9bb5111f4367 --auto-accept-changes main"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "preact",
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/",
      "public/"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@quickbaseoss/babel-plugin-styled-components-css-namespace": "^1.0.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "11.1.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/preact": "^6.4.9",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "aliasify": "^2.1.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-styled-components": "^2.0.2",
    "chromatic": "^6.3.3",
    "concurrently": "^7.0.0",
    "core-js": "^3.20.2",
    "css-loader": "^5.2.7",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^8.6.0",
    "eslint-config-preact": "^1.1.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "less": "^4.1.2",
    "less-loader": "^7.3.0",
    "less-watch-compiler": "^1.16.3",
    "preact-cli": "^3.0.0",
    "rollup": "2.35.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-import-css": "^3.0.2",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preact": "^0.5.2",
    "sirv-cli": "1.0.3",
    "storybook-preset-less": "^1.1.2",
    "style-loader": "^2.0.0"
  },
  "dependencies": {
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@storybook/addon-controls": "^6.4.9",
    "@types/styled-components": "^5.1.19",
    "preact": "^10.3.2",
    "preact-compat": "^3.19.0",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  },
  "aliasify": {
    "aliases": {
      "react": "preact-compat",
      "react-dom": "preact-compat"
    }
  }
}
